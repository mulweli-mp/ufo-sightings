{
	"expo": {
		"name": "UFO Sightings",
		"slug": "ufo-sightings",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/ufo_logo.png",
		"scheme": "ufosightings",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ufo.sightings",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/ufo_icon.png",
				"backgroundColor": "#261e67"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.ufo.sightings"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/ufo_logo.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#18e7f8"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "8da77336-0c1e-40ad-b9cd-377427ab7341"
			}
		}
	}
}
